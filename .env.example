# Firebase Emulator Configuration
GCP_PROJECT=kua-waiting-list-dev
APP_ENV=development

# Emulator Ports
EMULATOR_UI_PORT=4100
FIRESTORE_PORT=8282
AUTH_PORT=9190
STORAGE_PORT=9292
FUNCTIONS_PORT=5100

# Docker Image
DOCKER_IMAGE=kua-waiting-list:latest

# WhatsApp API Configuration
# Uncomment dan isi sesuai provider yang digunakan

# Untuk Fonnte
# WHATSAPP_API_URL=https://api.fonnte.com/send
# WHATSAPP_API_KEY=your_fonnte_api_key_here

# Untuk Wablas
# WHATSAPP_API_URL=https://console.wablas.com/api/send-message
# WHATSAPP_API_KEY=your_wablas_api_key_here

# Untuk WhatsApp Business API
# WHATSAPP_API_URL=https://graph.facebook.com/v18.0/YOUR_PHONE_NUMBER_ID/messages
# WHATSAPP_API_KEY=Bearer YOUR_ACCESS_TOKEN
